{
  "name": "emmet-keybindings",
  "displayName": "Emmet Keybindings",
  "description": "Improve your productivity through new keyboard shortcuts for Emmet.",
  "publisher": "agutierrezr",
  "author": {
    "name": "Andrés Gutiérrez"
  },
  "version": "1.1.0",
  "icon": "media/emmet-logo.png",
  "engines": {
    "vscode": "^1.50.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AGutierrezR/emmet-keybindings.git"
  },
  "keywords": [
    "html",
    "css",
    "emmet",
    "scss",
    "sass"
  ],
  "categories": [
    "Keymaps"
  ],
  "contributes": {
    "keybindings": [
      {
        "mac": "cmd+m cmd+i",
        "win": "ctrl+m ctrl+i",
        "key": "ctrl+m ctrl+i",
        "command": "editor.emmet.action.balanceIn",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+o",
        "win": "ctrl+m ctrl+o",
        "key": "ctrl+m ctrl+o",
        "command": "editor.emmet.action.balanceOut",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+e",
        "win": "ctrl+m ctrl+e",
        "key": "ctrl+m ctrl+e",
        "command": "editor.emmet.action.evaluateMathExpression",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+m",
        "win": "ctrl+m ctrl+m",
        "key": "ctrl+m ctrl+m",
        "command": "editor.emmet.action.matchTag",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+k",
        "win": "ctrl+m ctrl+k",
        "key": "ctrl+m ctrl+k",
        "command": "editor.emmet.action.nextEditPoint",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+j",
        "win": "ctrl+m ctrl+j",
        "key": "ctrl+m ctrl+j",
        "command": "editor.emmet.action.prevEditPoint",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+shift+m",
        "win": "ctrl+m ctrl+shift+m",
        "key": "ctrl+m ctrl+shift+m",
        "command": "editor.emmet.action.mergeLines",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+r",
        "win": "ctrl+m ctrl+r",
        "key": "ctrl+m ctrl+r",
        "command": "editor.emmet.action.reflectCSSValue",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+backspace",
        "win": "ctrl+m ctrl+backspace",
        "key": "ctrl+m ctrl+backspace",
        "command": "editor.emmet.action.removeTag",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+oem_period",
        "win": "ctrl+m ctrl+oem_period",
        "key": "ctrl+m ctrl+oem_period",
        "command": "editor.emmet.action.selectNextItem",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+oem_comma",
        "win": "ctrl+m ctrl+oem_comma",
        "key": "ctrl+m ctrl+oem_comma",
        "command": "editor.emmet.action.selectPrevItem",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+s",
        "win": "ctrl+m ctrl+s",
        "key": "ctrl+m ctrl+s",
        "command": "editor.emmet.action.splitJoinTag",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+/",
        "win": "ctrl+m ctrl+/",
        "key": "ctrl+m ctrl+/",
        "command": "editor.emmet.action.toggleComment",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+u",
        "win": "ctrl+m ctrl+u",
        "key": "ctrl+m ctrl+u",
        "command": "editor.emmet.action.updateImageSize",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+t",
        "win": "ctrl+m ctrl+t",
        "key": "ctrl+m ctrl+t",
        "command": "editor.emmet.action.updateTag",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+l",
        "win": "ctrl+m ctrl+l",
        "key": "ctrl+m ctrl+l",
        "command": "editor.emmet.action.wrapIndividualLinesWithAbbreviation",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+w",
        "win": "ctrl+m ctrl+w",
        "key": "ctrl+m ctrl+w",
        "command": "editor.emmet.action.wrapWithAbbreviation",
        "when": "editorTextFocus"
      },
      {
        "mac": "cmd+m cmd+c",
        "win": "ctrl+m ctrl+c",
        "key": "ctrl+m ctrl+c",
        "command": "workbench.action.showEmmetCommands",
        "when": "editorTextFocus"
      }
    ]
  }
}
